{% extends 'layout.twig' %}
{% block content %}
<div class="row">
{% for  snippet in snippets %}
    <section class="col-md-6">
        <h4 class="truncate"><a href="/snippet/{{snippet.id}}/{{snippet.title}}">{{snippet.title}}</a></h4>
        <p>{{snippet.description}}</p>
        <p>&nbsp;</p>
    </section>
{% endfor %}
</div>
{% endblock %}


